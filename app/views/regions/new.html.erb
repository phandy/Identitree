<h1>Create new Region</h1>
<p id="notice"><%= flash[:notice] %></p>
<%= form_for(@region) do |f| %>
	<div class="field>
		<%= f.label :region_name =%>
		<%= f.text_field :region_name =%>
	</div>
	<div class="field>
		<%= f.label :description =%>
		<%= f.text_field :description =%>
	</div>
	
	<%= f.submit %>
<% end %>